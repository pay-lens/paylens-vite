{
  "name": "paylens-vite",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start:vite": "vite",
    "dev": "node ./server/server",
    "build": "tsc && npm run build:client && npm run build:server",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --ssr src/entry-server.jsx --outDir dist/server",
    "debug": "node --inspect-brk server",
    "format": "yarn prettier --write .",
    "generate": "vite build --outDir dist/static && npm run build:server && node prerender",
    "lint:style": "stylelint-config-prettier-check",
    "lint:ts": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "optimize": "vite optimize",
    "postinstall": "husky install",
    "prepare": "husky install",
    "preview": "vite preview",
    "serve": "NODE_ENV=production node server",
    "stylelint-check": "stylelint-config-prettier-check",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "*.{ts,tsx,js,jsx}": "eslint --cache --fix",
    "*.css": "stylelint --fix",
    "*.{ts,tsx,js,jsx,css,md}": "prettier --write"
  },
  "dependencies": {
    "@emotion/core": "11.0.0",
    "@emotion/css": "11.11.2",
    "@emotion/react": "11.11.1",
    "@emotion/styled": "11.11.0",
    "@mui/material": "5.13.5",
    "compression": "^1.7.4",
    "express": "4.18.2",
    "express-session": "1.17.3",
    "normalize.css": "8.0.1",
    "react": "17.0.2",
    "react-dom": "18.2.0",
    "react-router-dom": "6.13.0",
    "serve-static": "1.15.0",
    "uuid": "9.0.0",
    "vite-plugin-svgr": "^3.2.0"
  },
  "devDependencies": {
    "@babel/cli": "7.22.5",
    "@babel/core": "7.22.5",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-proposal-optional-chaining": "7.21.0",
    "@babel/polyfill": "7.12.1",
    "@babel/preset-env": "7.22.5",
    "@babel/preset-react": "7.22.5",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/user-event": "14.4.3",
    "@types/jest-axe": "3.5.5",
    "@types/react": "18.0.37",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "@vitejs/plugin-react-swc": "3.0.0",
    "babel-jest": "29.5.0",
    "babel-loader": "9.1.2",
    "eslint": "^8.45.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-kentcdodds": "20.5.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-loader": "4.0.2",
    "eslint-plugin-flowtype": "8.0.3",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "27.2.2",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.3.4",
    "husky": "8.0.0",
    "jest-axe": "7.0.1",
    "jest-environment-jsdom": "29.5.0",
    "jest-svg-transformer": "1.0.0",
    "jsdom": "22.1.0",
    "prettier": "2.8.8",
    "prettier-eslint": "15.0.1",
    "prettier-stylelint": "0.4.2",
    "stylelint": "15.8.0",
    "stylelint-config-prettier": "9.0.5",
    "stylelint-config-standard-scss": "9.0.0",
    "typescript": "^5.1.6",
    "vite": "4.3.9",
    "vitest": "0.32.2"
  }
}
