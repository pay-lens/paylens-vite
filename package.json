{
  "name": "paylens-vite",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start:vite": "vite",
    "dev": "node ./server/server",
    "build": "tsc && npm run build:client && npm run build:server",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --ssr src/entry-server.jsx --outDir dist/server",
    "debug": "node --inspect-brk server",
    "format": "yarn prettier --write .",
    "generate": "vite build --outDir dist/static && npm run build:server && node prerender",
    "lint:style": "stylelint-config-prettier-check",
    "lint:ts": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "optimize": "vite optimize",
    "postinstall": "husky install",
    "prepare": "husky install",
    "preview": "vite preview",
    "serve": "NODE_ENV=production node server",
    "stylelint-check": "stylelint-config-prettier-check",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "*.{ts,tsx,js,jsx}": "eslint --cache --fix",
    "*.css": "stylelint --fix",
    "*.{ts,tsx,js,jsx,css,md}": "prettier --write"
  },
  "dependencies": {
    "@emotion/babel-plugin": "11.11.0",
    "@emotion/core": "11.0.0",
    "@emotion/css": "11.11.2",
    "@emotion/react": "11.11.1",
    "@emotion/styled": "11.11.0",
    "@loadable/component": "^5.15.3",
    "@mui/material": "5.14.2",
    "compression": "1.7.4",
    "express": "4.18.2",
    "express-session": "1.17.3",
    "normalize.css": "8.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.14.2",
    "serve-static": "1.14.1",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@babel/cli": "7.22.9",
    "@babel/core": "7.22.9",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-proposal-optional-chaining": "7.21.0",
    "@babel/preset-env": "7.22.9",
    "@babel/preset-react": "7.22.5",
    "@testing-library/jest-dom": "5.17.0",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/jest-axe": "3.5.5",
    "@types/react": "17.0.0",
    "@types/react-dom": "18.2.7",
    "@typescript-eslint/eslint-plugin": "6.2.0",
    "@typescript-eslint/parser": "6.2.0",
    "@vitejs/plugin-react": "4.0.3",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "babel-jest": "29.6.1",
    "babel-loader": "9.1.3",
    "eslint": "8.4.1",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-plugin-flowtype": "8.0.3",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jest": "27.2.3",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "eslint-plugin-react-refresh": "0.3.0",
    "eslint-webpack-plugin": "4.0.1",
    "husky": "7.0.4",
    "jest-axe": "8.0.0",
    "jest-environment-jsdom": "29.6.1",
    "jest-svg-transformer": "1.0.0",
    "jsdom": "22.1.0",
    "prettier": "3.0.0",
    "prettier-eslint": "15.0.1",
    "stylelint": "14.2.0",
    "stylelint-config-prettier": "9.0.5",
    "typescript": "5.1.6",
    "vite": "4.4.7",
    "vite-plugin-ssr": "0.4.133",
    "vite-plugin-style-import": "2.0.0",
    "vite-plugin-svg-icons": "2.0.1",
    "vite-plugin-svgr": "3.2.0",
    "vite-plugin-windicss": "1.9.0",
    "vite-ssg": "0.23.1",
    "vitest": "^0.33.0"
  },
  "peerDependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  }
}
